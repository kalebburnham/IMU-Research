<script>

function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        //return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        return results == null ? "" : results[1];
    }

function searchAction()
{
		        //event.preventDefault();
		        var url=window.location.href;
		        var currentQ=getParameterByName('q');
		        var odlParam="q="+currentQ;
		        var oldParam=getParameterByName('searchLabel');		
				
				if(url.indexOf("&q=") >=0)
					 {
								
							 	url = url.replace(odlParam,"q="+$("#searchText")[0].value);
							 	
					}
					else{
								url=url+"&q="+$("#searchText")[0].value;
					}
				 url=url.replace('searchLabel='+oldParam,'searchLabel=renderSearchPage');
		       	 window.location.href=url;
            
}
</script>

<!-- Added by b49843 for CR-59742 end -->
<script type="text/javascript">
		$(document).ready(function() {

			function add() {
			    if($(this).val() === ''){
			      $(this).val($(this).attr('placeholder')).addClass('placeholder');
			    }
			  }

			  function remove() {
			    if($(this).val() === $(this).attr('placeholder')){
			      $(this).val('').removeClass('placeholder');
			    }
			  }

			  // Create a dummy element for feature detection
			  if (!('placeholder' in $('<input>')[0])) {

			    // Select the elements that have a placeholder attribute
			    $('input[placeholder]').blur(add).focus(remove).each(add);
		    		 
			
			  }
				
		 });

		
</script>
