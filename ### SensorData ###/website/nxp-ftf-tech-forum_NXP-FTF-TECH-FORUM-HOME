<script>
$( "h1" ).text("NXP FTF TECH FORUM");
$( "h1" ).css("font-size","45px");
$( "h1" ).css("font-weight","bold");
$( "h1" ).css("text-align","center");

$(function(){
	var stickyHeaderTop = $('#main-menu').offset().top - 33;
	$(window).scroll(function(){
		if( $(window).scrollTop() > stickyHeaderTop ) {
			$('#wording').css({position: 'fixed', top: '0px'});
			$('#main-menu').css({position: 'fixed', top: '0px'});
			$('#menu-alias').css('display', 'block');
		} else {
			$('#wording').css({position: 'static', top: '0px'});
			$('#main-menu').css({position: 'static', top: '0px'});
			$('#menu-alias').css('display', 'none');
		}
	});
  });

$(document).ready(function(){
	var stickyHeaderTop = $('#main-menu').offset().top - 33;
	if($(window).scrollTop() > stickyHeaderTop)
	{
		$('#wording').css({position: 'fixed', top: '0px'});
		$('#main-menu').css({position: 'fixed', top: '0px'});
		$('#menu-alias').css('display', 'block');
	}
});
</script>
