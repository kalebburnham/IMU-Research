       <!--ls:begin[component-1473770942759]--><script type="text/javascript" src="http://maps.googleapis.com/maps/api/js"></script>
<style>
#map_wrapper {border:1px solid #C4CFD1; margin-bottom:14px; height:352ps; padding:0px;} 
#map_canvas {
    width: 748px;
    height: 350px;
    margin-right:auto;
    margin-left:auto;
    border:1px solid white;
    padding:1px;
    
}
.info-html {
    float:left;
    width:375px;
}
a.map_item{color: #51626F;}

#map_home {margin-bottom:4px;}

/*
#socialIcons img {height:28px;padding:1px;margin:2px}  
#socialIcons {margin:0px auto}*/
</style>

 <script language="JavaScript">
  
$(function(){
           
 var myOptions = {
      center: new google.maps.LatLng(30,15),
      zoom: 2,
      panControl: true,
      streetViewControl: false,
      mapTypeControl: true,
      zoomControl: true,
      zoomControlOptions: {
            style: google.maps.ZoomControlStyle.LARGE
        },
      //style: google.maps.MapTypeControlStyle.DROPDOWN_MENU,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    }
     var image = '/files/graphic/icons/G-orange-15x15.png';
     var hqimage = '/files/graphic/icons/G-green-25x25.png';
     var map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
     var infowindow = new google.maps.InfoWindow({content: "Location info" });
    //var infowindow = new google.maps.InfoWindow({pixelOffset: "20px" });

    $('a.map_item').each(function(){
                var lat1 = $(this).attr('lat');
                var longi1 = $(this).attr('longi');
                var locInfo1 = $(this).closest('div').html();
                var point1 = new google.maps.LatLng(lat1,longi1);
                
                if ($(this).attr('name1') == 'corphq') { 
                var marker1 = new google.maps.Marker({position:point1,map: map, icon: hqimage});
                }else{
                var marker1 = new google.maps.Marker({position:point1,map: map,icon: image});
				}
                  google.maps.event.addListener(marker1, 'click', function(){
                    infowindow.setContent(locInfo1);
                    infowindow.open(map,marker1);                
                    });         
    }); 

   $('a.map_item').click(function(e){
       
        var lat2 = $(this).attr('lat');
        var longi2 = $(this).attr('longi');
		var myLatLng = {lat: lat2, lng: longi2};

        var point2 = new google.maps.LatLng(lat2,longi2);
        map.setCenter(point2);
        map.setZoom(14);
        var locInfo2 = $(this).closest('div').html();
        infowindow.setContent(locInfo2);

    });
	
	/*Handling specific locations script*/
	var params={};
	window.location.search
	  .replace(/[?]+([^=&]+)=([^&]*)/gi, function(str,key,value) {
		params[key] = value;
	  }
	);

	if(params["location"] !== undefined){
		$("a[name='" + params["location"] + "']").trigger("click");
	}
});


</script> 

