            require([
                'jquery',
                'apps/pager/main',
                'domReady!'
            ], function($j, PagerMain) {
                var pager = new PagerMain($j('#widget-191629'),
                                                "/community/render-widget!execute.jspa #widget-191629 > *",
                                                {
                                                    frameID: 191629,
                                                    widgetType: 3,
                                                    widgetID: 1002,
                                                    container: 11497,
                                                    containerType: 14,
                                                    per_page: 25,
                                                    idPrefix: '191629',
                                                    size: 2
                                                },

                                                {
                                                    updateLocation: false
                                                });
                if (typeof(ContentFilterHandler_191629) != 'undefined') {
                    ContentFilterHandler_191629.contentLoader = pager;
                }
            });


        require([
            'jquery'
        ], function($) {
            if (typeof(ContentFilterHandler_191629) === "undefined") {
                ContentFilterHandler_191629 = {
                    params : { tag : null, tagSet : null },
                    contentLoader: { setStart: function(){}},
                    updateCategoryFilterCollapsedMessage: function(params) {
                        if(params.tagSet != null){
                            var id = "cat191629" + params.tagSet;
                            $('.jive-content-filtered-message.widgetclass_191629 .category').show();
                            var catval = $('#' + id).text();
                            catval = catval.substr(0, catval.lastIndexOf("("));
                            $('.jive-content-filtered-message.widgetclass_191629 .category span').text(catval);
                        }else{
                            $('.jive-content-filtered-message.widgetclass_191629 .category').hide();
                        }
                        if(params.tag != null){
                            var id = "tag191629" + params.tag;
                            $('.jive-content-filtered-message.widgetclass_191629 .tag').show();
                            var tagval = $('#' + id).text();
                            $('.jive-content-filtered-message.widgetclass_191629 .tag span').text(tagval);
                        }else{
                            $('#jive-content-filter-tag-name_191629').text('');
                            $('.jive-content-filtered-message.widgetclass_191629 .tag').hide();
                        }
                        if(params.tagSet != null && params.tag != null){
                            $('.jive-content-filtered-message.widgetclass_191629 .and').show();
                        }else{
                            $('.jive-content-filtered-message.widgetclass_191629 .and').hide();
                        }
                    },
                    toggleCategories : function() {
                            if ( $('.widgetclass_191629.jive-content-filter-body').is(':visible') ) {
                                this.closeCategoryFilter();
                            } else {
                                this.openCategoryFilter();
                            }
                            return false;
                        },
                    openCategoryFilter : function(){
                        $('.widgetclass_191629.jive-content-filtered-message').hide();
                        $('.widgetclass_191629.jive-content-filter-body').slideDown('fast');
                        $('.widgetclass_191629.jive-content-filter-hide').show();
                        $('.widgetclass_191629.jive-content-filter-show').hide();
                    },
                    closeCategoryFilter : function(){
                        this.updateCategoryFilterCollapsedMessage(this.params);
                        $('.widgetclass_191629.jive-content-filter-body').slideUp('fast');
                        $('.widgetclass_191629.jive-content-filter-hide').hide();
                        if ( $('.widgetclass_191629.jive-content-filter-body a').hasClass('jive-content-filter-highlight') ) {
                            $('.widgetclass_191629.jive-content-filtered-message').show();
                        }
                        $('.widgetclass_191629.jive-content-filter-show').show();
                    },
                    clearFilteredCategoryMessage : function() {
                            $('.widgetclass_191629.jive-content-filtered-message').hide();
                            $('.widgetclass_191629.jive-content-filter-body a').removeClass('jive-content-filter-highlight');
                            $(".widgetclass_191629.jive-content-filter-tags").hide();
                            $("#tags-all191629").show();
                            this.setContentParameters({ tagSet: null, tag: null });
                            this.closeCategoryFilter();
                            return false;
                        },
                    onCategoryClick : function(tagSetID) {
                        this.selectCategory(tagSetID);
                        return false;
                    },
                    selectCategory : function(tagSetID){
                        var id = "cat191629" + tagSetID;
                        if ( $('#' + id).hasClass('jive-content-filter-highlight') ) {
                            // Deselected a category
                            $('#' + id).removeClass('jive-content-filter-highlight');
                            $('.widgetclass_191629.jive-content-filter-tags a').removeClass('jive-content-filter-highlight');
                            $(".widgetclass_191629.jive-content-filter-tags").hide();
                            $('.widgetclass_191629.jive-table-row').show();
                            $('.widgetclass_191629.hidden').hide();
                            $("#tags-all191629").show();
                            this.setContentParameters({ tagSet: null, tag: null });
                        } else {
                            // Selected a category, highlight it and unhighlight any others
                            $('.widgetclass_191629.jive-content-filter-categories a').removeClass('jive-content-filter-highlight');
                            $('.widgetclass_191629.jive-content-filter-tags a').removeClass('jive-content-filter-highlight');
                            $('#' + id).addClass('jive-content-filter-highlight');
                            $(".widgetclass_191629.jive-content-filter-tags").hide();
                            $("#tags-" + id).show();
                            $("#tags-" + id).effect("highlight", {color: '#e4e9f3'}, 500);
                            $('.widgetclass_191629.jive-table-row').hide();
                            $(".widgetclass_191629.jive-table-recentcontent tbody tr").removeClass("jive-table-row-even");

                            $(".row-" + this.id).show();
                            $(".jive-table-recentcontent tbody tr.row-" + this.id + ":odd").addClass("jive-table-row-even");
                            this.setContentParameters({ tagSet: tagSetID, tag: null });
                        }
                        $("input[name='tag']").attr("value", "");
                    },
                    onTagClick: function(tag) {
                        var id = "tag191629" + tag
                        if (this.params.tagSet) {
                            id = id + this.params.tagSet;
                        }
                        if ( $("#" + id).hasClass('jive-content-filter-highlight') ) {
                            $("#" + id).removeClass('jive-content-filter-highlight');
                            this.setContentParameters({ tag: null, tagSet: this.params.tagSet });
                        } else {
                            $('.widgetclass_191629.jive-content-filter-tags-popular a').removeClass('jive-content-filter-highlight');
                            $("#" + id).addClass('jive-content-filter-highlight');
                            this.setContentParameters({ tag: tag, tagSet: this.params.tagSet });
                        }
                        return false;
                    },
                    // The new jive.Pager app accepts content parameters directly
                    // instead of reading from form input values.  However the form
                    // input and `setStart()` APIs need to be kept for
                    // compatibility for the time being.
                    setContentParameters: function(params) {
                        var k;
                        params = this.params = $.extend(this.params, params);
                        if (typeof(this.contentLoader.set_parameters) == 'function') {
                            this.contentLoader.set_parameters(params);
                        } else {
                            for (k in params) {
                                $('input[name=' + k + ']').attr('value', params[k] || '-1');
                            }
                            this.contentLoader.reloadListener = function(that){ return function(){
                                if (typeof(that.contentLoader.get_parameters) == 'function') {
                                    params = that.contentLoader.get_parameters();
                                }
                                this.updateCategoryFilterCollapsedMessage(this.params);
                            }}(this);
                            this.contentLoader.setStart(0);
                        }
                    }
                };
            }
        });





