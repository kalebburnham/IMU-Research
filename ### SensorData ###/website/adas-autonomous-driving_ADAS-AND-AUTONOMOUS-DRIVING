<script type="text/javascript">
	function videoAdjustment(){
		var w = $("#USDOT-SMART-CITY-CHALLENGE").width();
		var h = (w*9)/16;
		var bw = (w-156)/2;
		var bh = (h-105)/2;

		$("#USDOT-SMART-CITY-CHALLENGE").height(h);
		$("#USDOT-SMART-CITY-CHALLENGE .vjs-big-play-button").css("margin-left",bw);
		$("#USDOT-SMART-CITY-CHALLENGE .vjs-big-play-button").css("margin-top",bh);
		
	}
	
	videoAdjustment();
	//create_socialmedia();
	
	$(window).resize(function(){
		videoAdjustment();
	});
	
	/* Every time the window is scrolled ... */
    $(window).scroll( function(){
      var scrollPos = $(document).scrollTop();
        /* Check the location of each desired element */
        $('.hideme').each( function(i){
            var bottom_of_object = $(this).position().top + $(this).outerHeight();
            var bottom_of_window = $(window).scrollTop() + $(window).height();
            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window > bottom_of_object-100 ){
                $(this).animate({'opacity':'1'},750);
            }
        }); 

        if (scrollPos >= 135  ) { 
          $('#leftnav').css({'position':'fixed','top':'10px','z-index':'5'})
        } else {
          $('#leftnav').css({'position':'absolute','top':'auto','z-index':'5'})
        }
    
    });
	
	jQuery(document).ready(function($){
		var language;
		if($("#lang_cd").val() != "zh-Hans")
			language = "https://contact.nxp.com/SmartCitiesLP2016";
		else
			language = "https://contact.nxp.com/SmartCitiesLP2016-zh";
	
		$("#eloquaform-container").html('<iframe name="eloquaform" id="eloquaform" scrolling="no" src="' + language + '" frameborder="0" allowfullscreen=""  class="iframe-form"></iframe>');		
	
		$('#openeloquaform').click(function(e){
			$('.eloquaform-container').slideDown('slow');
			$(this).hide('slow');
		});
		
		//$('.grid').masonry();
		//$(".social-media-poc .row, .social-media-poc .grid").animate({'opacity':'1'},750);
	});
</script>
