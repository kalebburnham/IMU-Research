	$(document).ready(function(){
		if($('input#shortname').length > 0){
			if($('input#shortname').val().replace(/_/g,"")!=""){
				var shortname=$('input#shortname').val();
				var MCUMPU=$('input#shortname').val().substr(0,4)=="c731";
				var n=shortname.lastIndexOf('_');
				if(n==-1||!MCUMPU){
