<script>
                function  toggleSeries(sCode){
                                $('tr[id^="'+sCode+'_"]').toggle();
                                $('#a_'+sCode).toggleClass('dropDownactive');
                                
                }
                function showProdInfoPop(event,name){
                                $('.infoPop').hide();
                                var infoPopLeft = event.pageX+ event.currentTarget.offsetLeft-$("#leftnav.col-sm-3").width();
                                if (jQuery.support.leadingWhitespace) {               
                                                var infoPopTop = (event.currentTarget.offsetParent.offsetTop + event.currentTarget.offsetParent.offsetLeft+ event.currentTarget.offsetParent.offsetParent.offsetTop) -110;
                                }
                                else{
                                                var infoPopTop = (event.currentTarget.offsetParent.offsetTop + event.currentTarget.offsetParent.offsetLeft+ event.currentTarget.offsetParent.offsetParent.offsetTop)-220;
                                                
                                }
                                var infoPopTCont = $(window).scrollTop() + $(window).height() ;
                                var infoPopArrowTopDivStyle ={};
                                if ( (infoPopTop + 160 )> infoPopTCont)
                                
                                {
                                                var infodiff =  infoPopTop + 160 -infoPopTCont;
                                                var infoPopTop = infoPopTop - infodiff;
                                                var infoPopArrowTop =  infodiff -10 + "px";
                                                $('.infoPopArrow').css({"margin-top":infoPopArrowTop});
                                }
                                infoPopTop = infoPopTop +"px";
                                $('#infoPop_'+name).show();
                                $('#infoPop_'+name).css({"position": "absolute","min-width":"725px","left": infoPopLeft, "top": infoPopTop,  "background" : "#FFF", "z-index": "100", "padding" : "10px"});
                                /* setTimeout(function(){ 
                                                     $('#infoPop_'+name).hide();}, 5000); */
                };
                function hideProdInfoPop(name){

                                setTimeout(function(){ 
                                                $('#infoPop_'+name).hide();}, 3000);
                                
                }

                function showProdInfoDiv(name) {
                                $('#infoPop_'+name).show();
                }

                function hideProdInfoDiv(name) {
                                hideProdInfoPop(name);
                }

                function closeInfoPopDiv(name) {
                                $('#infoPop_'+name).hide();
                }
</script>
