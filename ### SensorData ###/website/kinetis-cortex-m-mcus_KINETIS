				}else{					
					var p_shortname=shortname.substring(0,n);
					if(document.getElementById("lang").value == "ko")
						$('a#ProdSelLink').attr("href","/ko/webapp/parametricSelector.sp#/&amp;c="+p_shortname+"&amp;c="+shortname+"&amp;cof=0&amp;am=0");
