<script>
$(document).ready(function() {
	var url = window.location.href;

		/* if (url.indexOf("?")>-1){
			url = url.substr(0,url.indexOf("?"));
		} */
		var jsonFileUrl=""+"/resources/scripts/nxp-cms/NewCoSectionalNavRules.json";
		
		var secNodeValue= $('#sechighlight').val();
		if(secNodeValue == 'noHighlight'){
		   $.getJSON(jsonFileUrl, function(data) {
			   $.each(data, function(index, element) {
	        	   if(url.match("^"+index) || url.indexOf(index)!=-1){
	            	   $('.'+element[0].Nodeid).css("background-color", "#ffffff");
	        		   $('.'+element[0].Nodeid+' > a ').css("color", "#777777");
	        		   return false;
	        	   }
	    	    }); 
	       });
	}
});

