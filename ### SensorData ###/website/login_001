	function deleteSessionCookie (cookieName, cookieValue) {
		document.cookie = escape(cookieName) + "=" + escape(cookieValue) + "; path=/";
		return true;
	} 
	function deleteCookieError(){
		deleteSessionCookie("RegisterError", "");	
	}
	deleteCookieError();
