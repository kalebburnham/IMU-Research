	<style>
		.cDiveTooltip {
			position: absolute;
		    display: none;
		    z-index: 10000;
		}
		.cDiveTooltipArrowLeft {
			left: 10% !important;
		}
		.cDiveTooltipArrowRight {
			left: 90% !important;
