	<script>
		var tooltipDisplayed=false;

	    function tooltipHoverIn(element, mEvent) {
		    
	        if(!tooltipDisplayed && element.getAttribute("tooltipText")!=""){

		        var tooltipCR=document.getElementById('iTooltipContainer');
		        var tooltipCT=document.getElementById('iTooltipContent');
		        var tooltipAR=document.getElementById('iTooltipArrow');

			    var box = element.getBoundingClientRect();
			     
			    var body = document.body;
			    var docElem = document.documentElement;

			    // (1)
			    var boxTop = box.top;
			    var boxLeft = box.left;
			     
			    // (2)
			    var scrollTop = window.pageYOffset || docElem.scrollTop || body.scrollTop;
			    var scrollLeft = window.pageXOffset || docElem.scrollLeft || body.scrollLeft;
			     
			    // (3)
			    var clientTop = docElem.clientTop || body.clientTop || 0;
			    var clientLeft = docElem.clientLeft || body.clientLeft || 0;
			     
			    // (4)
			    var top  = boxTop +  scrollTop - clientTop;
			    var left = boxLeft + scrollLeft - clientLeft;

			    tooltipCT.innerHTML		= element.getAttribute("tooltipText");
	        	tooltipCR.style.display	= 'block';
	        	top=top + element.scrollHeight;

	        	if(screen.width <= (left+tooltipCR.clientWidth)){
	        		left=left - (tooltipCR.clientWidth - element.offsetWidth) - 8;
	        		tooltipAR.className = tooltipAR.className.replace(/\bcDiveTooltipArrowLeft\b/,'cDiveTooltipArrowRight');
		        }

			    tooltipCR.style.left	= left + 'px';
		        tooltipCR.style.top		= top + 'px';
	        	
		        tooltipDisplayed=true;
	        }
	    }
	
	    function tooltipHoverOut(element,event) {

		    	var tooltipCR=document.getElementById('iTooltipContainer');
		    	tooltipCR.style.display = 'none';
	
		    	var tooltipAR=document.getElementById('iTooltipArrow');
		    	tooltipAR.className = tooltipAR.className.replace(/\bcDiveTooltipArrowRight\b/,'cDiveTooltipArrowLeft');
		    	
		        tooltipDisplayed=false;
	    }
	</script>
