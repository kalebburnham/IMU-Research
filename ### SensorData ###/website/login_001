	var s_lang_cd = "en";
	var s_formStart = "";
	if(s_lang_cd == null || s_lang_cd=="") {
		s_lang_cd = "en";
	}
	if(s_lang_cd != null && s_lang_cd != "" && s_lang_cd != "en" ) {
		s_lang_title = "Freescale+Sign+In+%7c+Register";
		if(s_lang_title.indexOf("Sign In") != -1) {
			s_lang_title = "NXP Sign In | Register";			
		}
	}
	else {
		s_lang_title = "Freescale+Sign+In+%7c+Register";
	}
	var pageLoadRule = "";
	
	pageLoadRule = pageLoadRule == "" ? "General":pageLoadRule;
	
	var s_currentUrl = getCleanPageURL();
	var s_contentFinder = getContentFinding(document.referrer, document.URL);	

	var digitalData={};
	digitalData.pageInfo = {};
	digitalData.userInfo = {};
	digitalData.siteInfo = {};
	digitalData.searchInfo = {};
	digitalData.formInfo = {}
   	digitalData.userInfo.fslVisitorID = s_vt_id;            
   	digitalData.siteInfo.sitePlatform = "full";
   	digitalData.siteInfo.lang = s_lang_cd;
   	digitalData.pageInfo.localTitle = s_lang_title;
   	digitalData.pageInfo.pageLoadRule = pageLoadRule;
   	digitalData.pageInfo.pageName = "login%3alogin";
   	digitalData.pageInfo.siteSection1 = "login";         	
   	digitalData.pageInfo.pageType = digitalData.pageInfo.pageName;
   	digitalData.pageInfo.pageURLClean = s_currentUrl; 
   	/*
    if(digitalData.pageInfo.pageName == "login:loginFailed") {
    	s.events='event29'; //~#
    } 
