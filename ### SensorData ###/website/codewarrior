            require([
                'jquery',
                'apps/pager/main',
                'domReady!'
            ], function($j, PagerMain) {
                var pager = new PagerMain($j('#widget-166786'),
                                                "/community/render-widget!execute.jspa #widget-166786 > *",
                                                {
                                                    frameID: 166786,
                                                    widgetType: 3,
                                                    widgetID: 1002,
                                                    container: 2031,
                                                    containerType: 14,
                                                    per_page: 5,
