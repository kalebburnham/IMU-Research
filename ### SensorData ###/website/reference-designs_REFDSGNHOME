<script>
var total_rows;

$(document).ready(function(){

    total_rows=$('tr.all:visible').length;
    $('.count').html('('+$('tr.all:visible').length+')');
    
    $("#byApp,#byProd").val('all');
    
    $("#globalFilter").keypress(function(e) {
        if (e.which == 13) {
            var keyword = $("#globalFilter").val();
            $("#swkeywordBtn").click();
        }
    });
    
    $("#swkeywordBtn").click(function() {
        var keyword = $("#globalFilter").val();
        $(".all").hide();
        if (keyword === null || keyword === '') {   
            $(".all").removeClass("kmatch");
            var Aval = $("#byApp").val();
            var Pval = $("#byProd").val();
            if(Aval == 'all' && Pval == 'all'){ $(".all").show();  }    
            if(Aval != 'all' && Pval != 'all'){ $(".SProd.SApp").show(); }else{ $(".SProd,.SApp").show(); }
        } else {
            $(".all").removeClass("kmatch");
            $('.all').each(function()
            {
                if($(this).find('.td2 .nxp-design-title').text().toLowerCase().indexOf(keyword.toLowerCase()) !== -1||
                $(this).find('.td2 .nxp-design-desc').text().toLowerCase().indexOf(keyword.toLowerCase()) !== -1)
                {
                    $(this).addClass('kmatch');
                }
            });
            var Aval = $("#byApp").val();
            var Pval = $("#byProd").val();
            if(Aval == 'all' && Pval == 'all'){ $(".all.kmatch").show();  }    
            if(Aval != 'all' && Pval != 'all'){ $(".SProd.SApp.kmatch").show(); }else{ $(".SProd.kmatch,.SApp.kmatch").show(); }
            trackpageaction("Inline search" ,"Inline Search",keyword);
        }
        if(total_rows==$('tr.all:visible').length)
            $('.count').html('('+$('tr.all:visible').length+')');
        else        
            $('.count').html('('+$('tr.all:visible').length+' of '+total_rows+')');
        var results = $('tr.all:visible').length;
        if(results <= 0){
            if($( "#AMsg" ).length <= 0){
                var html = "<tr id='AMsg'><td colspan='3'><div class='alert alert-warning'><p><span class='icon-warning2'></span> No matches found, check your filters</p></div></td></tr>";
                $(html).appendTo("#nxp-dsg tbody");
            }
        }else{
            if($("#AMsg").length){ $("#AMsg").remove(); }
        }
    });

    $("#byApp, #byProd").change(function(){
        var Aval = $("#byApp").val();
        var Pval = $("#byProd").val();
        var dDat = "mboxclick-nxpdesigns-app-"+Aval+"-prod-"+Pval+"";

        $(".all").hide();
        if(Aval == 'all'){ $(".all").removeClass("SApp"); }
        if(Pval == 'all'){ $(".all").removeClass("SProd"); }  
        if(Aval != 'all'){ $(".all").removeClass("SApp"); $("."+Aval).addClass("SApp"); }
        if(Pval != 'all'){ $(".all").removeClass("SProd"); $("."+Pval).addClass("SProd"); }
        
        $("#swkeywordBtn").trigger( "click" );  
        
        if(this.id == 'byApp'){
            trackpageaction("Application Filter Selection" ,"Filter Selection","Application: "+Aval);
        }else if(this.id == 'byProd'){
            trackpageaction("Product Filter Selection" ,"Filter Selection","Product: "+Pval);
        }
    });
});


digitalData.eventInfo = {};
function trackpageaction(sprop16,seVar57,seVar58){
    digitalData.pageInfo.contentFinding = "";
    digitalData.eventInfo.eventPathing=sprop16;
    digitalData.eventInfo.pageAction=seVar57;
    digitalData.eventInfo.pageSubaction=seVar58;
    _satellite.track("trackExternalPageAction");
    digitalData.pageInfo.contentFinding=digitalData.eventInfo.pageAction=digitalData.eventInfo.pageSubaction=digitalData.pageInfo.pageEventPathing=digitalData.eventInfo.eventPathing="";
}


</script>
