            ], function($j, PagerMain) {
                var pager = new PagerMain($j('#widget-166866'),
                                                "/community/render-widget!execute.jspa #widget-166866 > *",
                                                {
                                                    frameID: 166866,
                                                    widgetType: 3,
                                                    widgetID: 1002,
                                                    container: 11488,
                                                    containerType: 14,
                                                    per_page: 25,
                                                    idPrefix: '166866',
                                                    size: 2
                                                },

