<script type="text/javascript">
<!--Changes for ENHC0013516-->
if (!window._laq) {
    window._laq = [];
}
window._laq.push(
    function() {
        // Checking all availabilities, showing/hiding dummy elements
        // SFDC Definitions

        // Production
        var SFDC_Chat_Button_AP = '57357000000XZAo';
        var SFDC_Chat_Button_EMEA = '57357000000XZAp';
        var SFDC_Chat_Button_AMER = '57357000000XZAn';
        var SFDC_Chat_Button_EnglishFallBack = '57357000000XZAl';
        var SFDC_Chat_Button_Availability_All = '57357000000XZAm';
        var SFDC_Chat_Button_Availability_ACHN = '573D0000000TORo';

        // Internal invisible indicators need to be set based on individual queue availability
        // EMEA
        liveagent.showWhenOnline(SFDC_Chat_Button_EMEA, document.getElementById('liveagent_button_online_EMEA'));
        // AMER
        liveagent.showWhenOnline(SFDC_Chat_Button_AMER, document.getElementById('liveagent_button_online_AMER'));
        // AP EN
        liveagent.showWhenOnline(SFDC_Chat_Button_AP, document.getElementById('liveagent_button_online_AP'));
        //                                // English Fallback
        liveagent.showWhenOnline(SFDC_Chat_Button_EnglishFallBack, document.getElementById('liveagent_button_online_EnglishFallBack'));

        //Showing/hiding real buttons
        // General Availability
        if (!!document.getElementById('webchatbutton_on')) {
            liveagent.showWhenOnline(SFDC_Chat_Button_Availability_All, document.getElementById('webchatbutton_on'));
            liveagent.showWhenOffline(SFDC_Chat_Button_Availability_All, document.getElementById('webchatbutton'));
        }
        // AP CHN
        if (!!document.getElementById('webchatbutton_chn_on')) {
            liveagent.showWhenOnline(SFDC_Chat_Button_Availability_ACHN, document.getElementById('webchatbutton_chn_on'));
            liveagent.showWhenOffline(SFDC_Chat_Button_Availability_ACHN, document.getElementById('webchatbutton_chn_off'));
        }
    });
</script>
<!--End of Changes for ENHC0013516-->

