<script type='text/javascript' src='https://c.la1-c2cs-frf.salesforceliveagent.com/content/g/js/38.0/deployment.js'></script>
<!--START JavaScript for AJAX Calls in live Web Chat -->
<script type="text/javascript">
//Added by b31782 on 10 Apr 2011 start
function init() {
    var webPath = getWebPath();
    //var strURL = webPath + "//www.nxp.com/webapp/intranet_admin/checkAvailableChatHours.jsp";
	var strURL = "//www.nxp.com/webapp/intranet_admin/checkAvailableChatHours.jsp";
    sendRequest("get", strURL, "level0");
}

window.onload = init;

String.prototype.trim = function() {
    return this.replace(/(^\s*)/g, "").replace(/\&/g, '&amp;');
}

function sendRequest(method, url, dId) {
    if (method == 'get' || method == 'GET') {
        var http = null;
        if (window.XMLHttpRequest) {
            http = new XMLHttpRequest();
        } else if (window.ActiveXObject) {
            http = new ActiveXObject("Microsoft.XMLHTTP");
        }

        if (http != null) {
            http.onreadystatechange = function() {
                if (http.readyState == 4) {
                    if (http.status == 200) {
                        try {
                            var response = http.responseText;


                            var result = response.indexOf("true");

                            //  document.write(response);
                        } catch (responseErr) {
                            alert("Error While retrieving Data..");

                        }
                        var s1 = "true";

                        if (result > -1) {

                            showAvailable();

                        } else {

                            ShowUnavailable();
                        }
                    }
                }
            };
            http.open(method, url);
            http.send(null);
        }
    }
}

function getWebPath() {
    var url = window.location.href;
    var whttp = url.split('//')[0];
    var nohttp = url.split('//')[1];
    var hostPort = nohttp.split('/')[0];
    return (whttp + "//" + hostPort);
}
<!-- Changes for ENHC0013516-->
//liveagent.init('https://d.la1-c2cs-frf.salesforceliveagent.com/chat', '572D0000000TOQq', '00D4E0000000TLy');
liveagent.init('https://d.la1-c2-frf.salesforceliveagent.com/chat', '572D0000000TOQq', '00D20000000NBgb');

function CheckButtonsThenStartChat(startchat) {
    // Timezone definitions
    var d = new Date();
    var AP_start = 1;
    var AP_end = 7;
    var Overlap_AP_EMEA_start = 7;
    var Overlap_AP_EMEA_end = 9;
    var EMEA_start = 9;
    var EMEA_end = 13;
    var Overlap_EMEA_AMER_start = 13;
    var Overlap_EMEA_AMER_end = 15;
    var AMER_start = 15;
    var AMER_end = 23;
    var Overlap_AMER_AP_start_1 = 23; // before UTC midnight
    var Overlap_AMER_AP_end_1 = 25;
    var Overlap_AMER_AP_start_2 = 0; // after UTC midnight
    var Overlap_AMER_AP_end_2 = 1;
    var h_UTC = d.getUTCHours();
    var my_h_UTC = "";
    var calendar_availability = "";

    // SFDC Definitions

    // Production
    var SFDC_Chat_Button_AP = '57357000000XZAo';
    var SFDC_Chat_Button_EMEA = '57357000000XZAp';
    var SFDC_Chat_Button_AMER = '57357000000XZAn';
    var SFDC_Chat_Button_EnglishFallBack = '57357000000XZAl';
    var SFDC_Chat_Button_Availability_All = '57357000000XZAm';
    var SFDC_Chat_Button_Availability_ACHN = '573D0000000TORo';

    // Temp variables to make the logic better readable
    var EMEA_available, AMER_available, AP_available, EnglishFallback_available, Someone_available;
    var EMEA_only, AMER_only, AP_only, Overlap, Overlap_AP_EMEA, Overlap_EMEA_AMER, Overlap_AMER_AP;
    var SFDC_Chat_Button_Used = '';
    var SFDC_Chat_Button_Used_Debug = '';


    // Get availability of subteams based on logged-in agents checking status as set by SFDC routine
    AP_status_available = (document.getElementById('liveagent_button_online_AP').style.display !== "none");
    EMEA_status_available = (document.getElementById('liveagent_button_online_EMEA').style.display !== "none");
    AMER_status_available = (document.getElementById('liveagent_button_online_AMER').style.display !== "none");
    EnglishFallBack__status_available = (document.getElementById('liveagent_button_online_EnglishFallBack').style.display !== "none");

    // Define which team should be current priority team
    AP_only = (h_UTC >= AP_start) && (h_UTC < AP_end);
    EMEA_only = (h_UTC >= EMEA_start) && (h_UTC < EMEA_end);
    AMER_only = (h_UTC >= AMER_start) && (h_UTC < AMER_end);
    Overlap_AP_EMEA = ((h_UTC >= Overlap_AP_EMEA_start) && (h_UTC < Overlap_AP_EMEA_end));
    Overlap_EMEA_AMER = ((h_UTC >= Overlap_EMEA_AMER_start) && (h_UTC < Overlap_EMEA_AMER_end));
    Overlap_AMER_AP = ((h_UTC >= Overlap_AMER_AP_start_1) && (h_UTC < Overlap_AMER_AP_end_1)) || ((h_UTC >= Overlap_AMER_AP_start_2) && (h_UTC < Overlap_AMER_AP_end_2));
    Overlap = Overlap_AP_EMEA || Overlap_EMEA_AMER || Overlap_AMER_AP;


    // Now decide which queue to use

    if (AP_only) {
        if (AP_status_available) {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_AP;
        } else {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_EnglishFallBack;
        }
    }


    if (EMEA_only) {
        if (EMEA_status_available) {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_EMEA;
        } else {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_EnglishFallBack;
        }
    }

    if (AMER_only) {
        if (AMER_status_available) {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_AMER;
        } else {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_EnglishFallBack;
        }
    }

    if (Overlap) {
        if (EnglishFallBack__status_available) {
            SFDC_Chat_Button_Used = SFDC_Chat_Button_EnglishFallBack;
        }
    }


    if (startchat == "EN") {
        liveagent.startChat(SFDC_Chat_Button_Used);
    }
    if (startchat == "CHN") {
        liveagent.startChat(SFDC_Chat_Button_Availability_ACHN);
    }


}
</script>
<!-- End Changes for ENHc0013516-->
<!--END JavaScript for AJAX Calls in Live Web chat -->
<!--START JavaScript for Main Logic written for the Sales and support page For live Web Chat tab-->
<script type="text/javascript">
<!--Changes for ENHC0013516-->
function showAvailable() {
    // This routine modifies the text of the Unavailable button from permanently unavailable to temporarily unavailable
    // The button is only shown if the liveagent routines switches it on
    // General Availability
    if (!!document.getElementById('webchatbutton')) {
        document.getElementById("webchatbutton").innerHTML = '<a  title="Available Worldwide, Monday through Friday during local business hours"><strong>Live Chat:<br>(All agents are busy,<br>please try again later)</strong></a>';
    }
    // AP CHN

    // if (!!document.getElementById('webchatbutton_chn_off')) { 
    //     document.getElementById("webchatbutton_chn_off").innerHTML = '<a href="http://www.nxp.com/support/communities:COMMUNITIES"><button class="btn btn-block" title="????:?????,??9????5?(????"><span class="icon-bubbles"></span>&nbsp;??????<br />???,???</button></a>';
    //}
}

function ShowUnavailable() {
    // This routine modifies the text of the Unavailable button from temporarily unavailable to permanently unavailable
    // The button is only shown if the liveagent routines switches it on
    if (!!document.getElementById('webchatbutton')) {
        document.getElementById("webchatbutton").innerHTML = '<a  href="https://community.nxp.com/" title="Available Monday-Friday, 8AM-6PM CT (US & Canada),  9AM-5PM CET (Europe), 9AM-5PM HKG (Asia)."><strong>Live Chat: Offline</strong></a>';
    }
    // AP CHN
    //if (!!document.getElementById('webchatbutton_chn_off')) {
    //  document.getElementById("webchatbutton_chn_off").innerHTML = '<a href="http://www.nxp.com/support/communities:COMMUNITIES"><button class="btn btn-block" title="????:?????,??9????5?(????"><span class="icon-bubbles"></span>&nbsp;??????<br />??</button></a>';
    //}
}
</script>
